{
  "name": "reablocks",
  "version": "5.8.6",
  "description": "Component library for React",
  "scripts": {
    "build": "pnpm --recursive run build",
    "build-storybook": "storybook build",
    "copy": "cp CNAME storybook-static/",
    "lint": "eslint --ext js,ts,tsx",
    "lint:fix": "eslint --ext js,ts,tsx --fix src",
    "lint:prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'",
    "start": "storybook dev -p 9009",
    "test": "vitest --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "prepare": "husky install"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "browser": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reaviz/reablocks.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "tree"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/reaviz/reablocks/issues"
  },
  "homepage": "https://github.com/reaviz/reablocks#readme",
  "dependencies": {
    "@marko19907/string-to-color": "^1.0.0",
    "@reaviz/react-use-fuzzy": "^1.0.3",
    "chroma-js": "^2.4.2",
    "classnames": "^2.3.1",
    "coverup": "^0.1.1",
    "create-global-state-hook": "^0.0.2",
    "date-fns": "^2.30.0",
    "ellipsize": "^0.5.1",
    "focus-trap-react": "^8.7.1",
    "framer-motion": "^10.16.16",
    "fuse.js": "^6.6.2",
    "human-format": "^1.2.0",
    "mousetrap": "^1.6.5",
    "name-initials": "^0.1.3",
    "pluralize": "^8.0.0",
    "rdk": ">=6.6.0",
    "react-18-input-autosize": "^3.0.0",
    "react-fast-compare": "^3.2.2",
    "react-highlight-words": "^0.20.0",
    "react-textarea-autosize": "^8.5.3"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^7.4.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-mdx-gfm": "^7.4.6",
    "@storybook/addon-storysource": "^7.4.6",
    "@storybook/addons": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-vite": "^7.4.6",
    "@storybook/theming": "^7.4.6",
    "@types/classnames": "^2.3.1",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-c8": "^0.33.0",
    "autoprefixer": "^10.4.16",
    "chromatic": "^7.4.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.1",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.2.0",
    "prettier": "^3.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.47.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "storybook": "^7.4.6",
    "typescript": "^4.9.5",
    "vite": "^4.4.11",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 80
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
