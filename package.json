{
  "name": "reablocks",
  "version": "9.3.0",
  "description": "Component library for React",
  "scripts": {
    "build": "npm run build:js && npm run build:styles && npm run rewrite:stories && npm run build:docs",
    "build-storybook": "storybook build",
    "build:js": "vite build --mode library",
    "build:styles": "npx @tailwindcss/cli -i ./src/index.css -o ./dist/index.css",
    "build:docs": "node scripts/docs.js",
    "rewrite:stories": "node scripts/stories.cjs",
    "lint": "eslint --ext js,ts,tsx",
    "lint:fix": "eslint --ext js,ts,tsx --fix src",
    "lint:prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'",
    "start": "storybook dev -p 9009",
    "test": "vitest --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "prepare": "husky install"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./index.css": "./dist/index.css",
    "./stories/*": "./dist/stories/*",
    "./blocks/*": "./dist/blocks/*"
  },
  "browser": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reaviz/reablocks.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "component library",
    "ui components",
    "tailwind",
    "tailwind css",
    "tailwind components",
    "tailwind elements",
    "tailwind library"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/reaviz/reablocks/issues"
  },
  "homepage": "https://github.com/reaviz/reablocks#readme",
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@reaviz/react-use-fuzzy": "^1.0.3",
    "body-scroll-lock-upgrade": "^1.1.0",
    "chroma-js": "^3.1.2",
    "classnames": "^2.5.1",
    "coverup": "^0.1.1",
    "create-global-state-hook": "^0.0.2",
    "ctrl-keys": "^1.0.6",
    "date-fns": "^4.1.0",
    "ellipsize": "^0.6.2",
    "focus-trap-react": "^10.3.1",
    "fuse.js": "^6.6.2",
    "human-format": "^1.2.1",
    "motion": "^12.23.12",
    "name-initials": "^0.1.3",
    "pluralize": "^8.0.0",
    "react-fast-compare": "^3.2.2",
    "react-highlight-words": "^0.21.0",
    "react-textarea-autosize": "^8.5.9",
    "tailwind-merge": "^2.6.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-interactions": "^9.0.0-alpha.10",
    "@storybook/addon-mdx-gfm": "^9.0.0-alpha.13",
    "@storybook/addon-storysource": "^9.0.0-alpha.14",
    "@storybook/addon-themes": "^9.1.5",
    "@storybook/manager-api": "^9.0.0-alpha.1",
    "@storybook/preview-api": "^9.0.0-alpha.1",
    "@storybook/react": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@storybook/theming": "^9.0.0-alpha.1",
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@types/classnames": "^2.3.4",
    "@types/node": "^22.18.1",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.1.5",
    "fast-glob": "^3.3.3",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.3.1",
    "prettier": "^3.6.2",
    "react": "^18.0.0",
    "react-docgen-typescript": "^2.4.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.62.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "storybook": "^9.1.5",
    "tailwindcss": "^4.1.13",
    "tw-colors": "^4.0.0-beta.1",
    "typescript": "^5.9.2",
    "typescript-rewrite-paths": "^1.3.1",
    "vite": "^7.1.5",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.2",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 80
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext js,ts,tsx --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": ">=22",
    "npm": ">=10.8.2"
  }
}
